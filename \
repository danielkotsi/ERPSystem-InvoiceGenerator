package utils

import (
	"-invoice_manager/internal/backend/models"
	"fmt"
	"net/http"
)

func GetCustomerFormData(r *http.Request) (customer models.Customer, err error) {

	if err := r.ParseMultipartForm(20000000); err != nil {
		return customer, nil
	}

	fmt.Println(len(r.Form))
	return customer, err
}

